<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
			<class name="com.qhit.lh.g4.ZWK.T01.bean.User"  table = "tb_user">
					<id name="uid" column="uid" type="integer">
							<generator class="native">							
							</generator>
					</id>
					<property name="uname" column="uname" type="String"></property>
					<property name="upwd"  column="upwd" type="String"></property>
					<property name="sex" column="sex" type="String"></property>
					<property name="birthday" column="birthday" type="String"></property>
					<property name="enable" column="enable" type="String"></property>
			</class>	
</hibernate-mapping>
